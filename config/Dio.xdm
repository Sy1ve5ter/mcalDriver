    <d:ctr type="AUTOSAR" factory="autosar" 
         xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd" 
         xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd" 
         xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd" >
        <d:lst type="TOP-LEVEL-PACKAGES">
            <d:ctr name="TS_T40D2M10I2R0" type="AR-PACKAGE">
                <a:a name="UUID" value="ECUC:7690b8cf-35e0-40d4-8b90-603d098bfa1b"/>
                <d:lst type="ELEMENTS">
                    <d:chc name="Dio" type="AR-ELEMENT" value="MODULE-DEF">
                        <v:ctr type="MODULE-DEF">
                            <a:a name="RELEASE" value="asc:4.3"/>
                            <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                                <ad:ADMIN-DATA>
                                    <ad:DOC-REVISIONS>
                                        <ad:DOC-REVISION>
                                            <ad:REVISION-LABEL>4.6.0</ad:REVISION-LABEL>
                                            <ad:ISSUED-BY>AUTOSAR</ad:ISSUED-BY>
                                            <ad:DATE>2014-10-31</ad:DATE>
                                        </ad:DOC-REVISION>
                                    </ad:DOC-REVISIONS>
                                </ad:ADMIN-DATA>
                            </a:a>
                            <a:a name="POSTBUILDVARIANTSUPPORT" value="true"/>
                            <a:a name="DESC">
                                <a:v>
                                    <![CDATA[EN:
                                        <html><p> Configuration of the Dio (Digital IO) module. </p></html>
                                    ]]>
                                </a:v>
                            </a:a>
                            <a:a name="LOWER-MULTIPLICITY" value="1"/>
                            <a:a name="UPPER-MULTIPLICITY" value="1"/>
                            <a:a name="UUID" value="ECUC:68e60706-b10b-4b09-8ae8-3bec4104934c"/>
                            <!--  @implements IMPLEMENTATION_CONFIG_VARIANT_Object  -->
                            <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                                <a:a name="LABEL" value="Config Variant"/>
                                <a:a name="UUID" value="ECUC:1a6b36f9-eb4c-46d6-87b5-9360aa3b226c"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html><p>
                                                VariantPreCompile: Only precompile time configuration parameters.<br>
                                                The files Dio_Cfg.h and Dio_Cfg.c are used.<br>
                                            </p></html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantLinkTime</icc:v>
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:da name="DEFAULT" value="VariantPreCompile"/>
                                <a:da name="RANGE">
                                  <a:v>VariantLinkTime</a:v>
                                  <a:v>VariantPreCompile</a:v>
                                </a:da>
                            </v:var>
                            <!--  @implements DioConfig_Object  -->
                            <v:ctr name="DioConfig" type="IDENTIFIABLE">
                                <a:a name="DESC" 
                                     value="EN: This container contains the configuration parameters and sub containers of the AUTOSAR DIO module. This container is a MultipleConfigurationContainer, i.e. this container and its sub-containers exist once per configuration set."/>
                                <a:a name="UUID" value="ECUC:4e1b9d37-58b0-46ba-a549-c40c8e80d6f3"/>
                                <!--  @implements DioPort_Object  -->
                                <v:lst name="DioPort" type="MAP">
                                    <a:da name="MIN" value="1"/>
                                    <v:ctr name="DioPort" type="IDENTIFIABLE">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:
                                                    <html><p>
                                                        Configuration of individual DIO ports, consisting of channels and possible channel groups.
                                                        The single DIO channel levels inside a DIO port represent a bit in the DIO port value. A channel group is a formal
                                                        logical combination of several adjoining DIO channels within a DIO port. The configuration process for Dio module
                                                        shall provide symbolic names for each configured DIO channel, port and group.
                                                    </p></html>
                                                ]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v mclass="PreCompile">VariantLinkTime</icc:v>
                                                <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                        </a:a>                                        
                                        <a:a name="UUID" value="ECUC:4e6fabe5-32b5-42b8-b166-115c11cdc751"/>
                                        <!--  @implements DioPortId_Object  -->
                                        <v:var name="DioPortId" type="INTEGER">
                                            <a:a name="LABEL" value="Dio Port Id"/>
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:
                                                        <html><p>
                                                            Numeric identifier of the DIO port. Symbolic names will be generated for each port pin id
                                                            for the pins which being used for configuration.
                                                            NOTE: Use the following values to configure different ports.
                                                            <ul>
                                                                <li>PortA=0</li>
                                                                <li>PortB=1</li>
                                                                <li>PortC=2</li>
                                                                <li>PortD=3</li>
                                                                <li>PortE=4</li>
                                                            </ul>
                                                        </p></html>
                                                    ]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                                 type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v vclass="Link">VariantLinkTime</icc:v>
                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                            <a:a name="SCOPE" value="ECU"/>
                                            <a:a name="UUID" value="ECUC:e572b945-92b2-4a6b-9209-16791c1307cc"/>
                                            <a:da name="DEFAULT" value='0'/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&lt;=num:i(count(ecu:list('Dio.AvailablePortPinsForRead')) - 1)"/>
                                                <a:tst expr="&gt;=0"/>
                                            </a:da>
                                            <a:da name="RANGE" type="XPath">
                                                <a:tst expr="text:uniq(../../*/DioPortId, .)"
                                                       false="DioPortId is repeated for two or more ports."/>
                                                <a:tst expr="(num:hextoint(ecu:list('Dio.AvailablePortPinsForWrite')[num:dectoint(node:current())+1]) != num:i('0')) or (num:hextoint(ecu:list('Dio.AvailablePortPinsForRead')[num:dectoint(node:current())+1]) != num:i('0'))"
                                                       false="This port is not available for the selected package. Please see RM for a list of available ports for selected package."/>
                                            </a:da>
                                        </v:var>
                                        <!--  @implements DioChannel_Object  -->
                                        <v:lst name="DioChannel" type="MAP">
                                            <v:ctr name="DioChannel" type="IDENTIFIABLE">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:
                                                            <html><p>
                                                                Configuration of an individual DIO channel. Symbolic names will be generated for each channel.
                                                                A general purpose digital IO pin represents a DIO channel which will be having value either STD_HIGH or STD_LOW.
                                                            </p></html>
                                                        ]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v mclass="PreCompile">VariantLinkTime</icc:v>
                                                        <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                                </a:a>
                                                <a:a name="UUID" value="ECUC:27aaf24e-fe6b-4d85-9666-e20239723ac8"/>
                                                <!--  @implements DioChannelId_Object  -->
                                                <v:var name="DioChannelId" type="INTEGER">
                                                    <a:a name="LABEL" value="Dio Channel Id"/>
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html><p>
                                                                    Channel Id of the DIO channel. This value will be assigned to the symbolic names.
                                                                </p></html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="Link">VariantLinkTime</icc:v>
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                                    <a:a name="SCOPE" value="ECU"/>                                                    
                                                    <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:0222ad95-d800-476d-a551-96fcf5c84481"/>
                                                    <a:da name="DEFAULT" value='0'/>
                                                    <a:da name="INVALID" type="Multi">
                                                      <mt:xpath expr="text:uniq(../../*/DioChannelId, .)"
                                                               false="DioChannelId is repeated for two or more channels."/>
                                                       <mt:xpath expr="(bit:and(num:hextoint(ecu:list('Dio.AvailablePortPinsForWrite')[num:dectoint((node:current())/../../../DioPortId)+1]),bit:shl( num:i(1),num:dectoint ( node:current() ) )) != '0') or (bit:and(num:hextoint(ecu:list('Dio.AvailablePortPinsForRead')[num:dectoint((node:current())/../../../DioPortId)+1]),bit:shl( num:i(1),num:dectoint ( node:current() ) )) != '0')"
                                                               false="This channel is not available in the selected package. Please see RM for a list of available channels for selected package."/>
                                                      <mt:range> <mt:tst expr="&gt;=0" /></mt:range>
                                                        <mt:range> <mt:tst expr="&lt;=31" /></mt:range>
                                                    
                                                    </a:da>
                                                </v:var>
                                            </v:ctr>
                                        </v:lst>
                                        <!--  @implements DioChannelGroup_Object  -->
                                        <v:lst name="DioChannelGroup" type="MAP">
                                            <a:da name="MAX" value="255"/>
                                            <v:ctr name="DioChannelGroup" type="IDENTIFIABLE">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:
                                                            <html><p>
                                                                A channel group represents several adjoining DIO channels represented by a logical group.
                                                                This container definition does not explicitly define a symbolic name parameter, but symbolic names will be generated for
                                                                each channel group. Each group provides a structure with parameters port, offset and mask.
                                                            </p></html>
                                                        ]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v mclass="PreCompile">VariantLinkTime</icc:v>
                                                        <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                                </a:a>
                                                <a:a name="UUID" value="ECUC:39154b03-0601-4bfd-8f96-5ec46a6543ea"/>
                                                <!--  @implements DioChannelGroupIdentification_Object  -->
                                                <v:var name="DioChannelGroupIdentification" type="STRING">
                                                    <a:a name="LABEL" value="Dio Channel Group Identification"/>
                                                    <a:a name="DESC">  
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html><p>
                                                                    A DIO channel group is identified in DIO APIs by a pointer to a data structure of type Dio_ChannelGroupType. This data structure
                                                                    contains the channel group information. This parameter contains the code fragment that has to be inserted in the API call of the calling module to get the address
                                                                    of the variable in memory which holds the channel group information, a string value should be given for this parameter. Symbolic names will be generated for
                                                                    each DioChannelGroup, which will be assigned with address of this string inorder to point to the structure parameters. Example: OutputGroup
                                                                </p></html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantLinkTime</icc:v>
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                                    <a:a name="SCOPE" value="ECU"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:0222ad95-d800-476d-a551-96fcf5c84482"/>
                                                    <a:da name="DEFAULT" value="DioChannelGroup"/>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="text:match(normalize-space(.),'^[_a-zA-Z]+[_0-9a-zA-Z]*$') or normalize-space(.) = ' ' " false="DioChannelGroupIdentification name should be entered as string."/>
                                                        <a:tst expr="text:uniq(../../../..//DioChannelGroup//DioChannelGroupIdentification, .)" false="DioChannelGroupIdentification is repeated for two or more channel groups. Please enter different symbolic name."/>
                                                    </a:da>
                                                </v:var>
                                               
                                                <!--  @implements DioPortBitNumber_Object  -->
                                                <v:var name="DioPortBitNumber" type="INTEGER">
                                                <a:a name="LABEL" value="Dio Port Bit Number"/>
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html><p>
                                                                 This is the number of continuous channels that create a channel group
                                                                </p></html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                     <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                                         type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantLinkTime</icc:v>
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:49aebdd1-b2b7-42e8-82fc-c09f8321978b"/>
                                                    <a:da name="DEFAULT" value='1'/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&gt;=0"/>
                                                        <a:tst expr="&lt;=32"/>
                                                    </a:da>
                                                </v:var> 
                                                
                                                  <!--  @implements DioPortOffset_Object  -->                                                    
                                                <v:var name="DioPortOffset" type="INTEGER">
                                                <a:a name="LABEL" value="Dio Port Offset"/>
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html><p>
                                                                 The position of the Channel Group on the port, counted from the least significant bit. This value can be derived from DioPortMask.
                                                                 calculationFormula = Position of the first bit of DioPortMask which is set to 1 counted from least significant bit
                                                                </p></html> 
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="Link">VariantLinkTime</icc:v>
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:0222ad95-d800-476d-a551-96fcf6c84482"/>
                                                      <a:da name="DEFAULT" value='0'/>                                                          
                                                      <a:da name="INVALID" type="Multi">
                                                       <mt:xpath expr="(node:fallback(.,0) &gt;= 0) and (node:fallback(.,0) &lt;= (num:i(32-node:fallback(../DioPortBitNumber, '1'))))"
                                                        false="Value out of range: "/>
                                                        <mt:range> <mt:tst expr="&gt;=0" /></mt:range>
                                                        <mt:range> <mt:tst expr="&lt;=31" /></mt:range>
                                                      
                                                      </a:da>
                                                </v:var>                                                    
                                                
                                                <!--  @implements DioPortMask_Object  -->
                                                <v:var name="DioPortMask" type="INTEGER">
                                                    <a:a name="LABEL" value="Dio Port Mask"/>
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html><p>
                                                                This shall be the mask which defines the positions of the channel group.
                                                                This field holds a decimal value that, when converted into binary, represents a bitmask for the channels in the group(e.g a value of 14 = 0x0E selects channels 1, 2 and 3).
                                                                The binary value of the mask should have a single continuous group of 1 bits.
                                                                The data type depends on the port width.
                                                                To display the correct result you must click on the "calculator" icon near the Dio Port Mask field
                                                                Calculation formula: DioPortMask=DioPortBitNumber<<DioPortOffset
                                                                </p></html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="Link">VariantLinkTime</icc:v>
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    </a:a>
                                                   
                                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:a13389fa-d994-47e4-99d7-4cfaea7c4f1d"/>

                                                     <a:da name="DEFAULT" type="XPath">                            
                                                        <a:tst expr="
                                                                    ( 
                                                                         bit:shl((bit:shl(1,node:fallback(../DioPortBitNumber,0)) - 1), node:fallback(../DioPortOffset,1))
                                                                    )
                                                        "/> 
                                                    </a:da>

                                                    <a:a name="INVALID" type="Multi">
                                                       <mt:xpath expr="
                                                        (
                                                            not( node:empty( node:fallback(., 1) )) and 
                                                            (
                                                                num:i(node:fallback(., 1)) != 
                                                                (
                                                                      bit:shl((bit:shl(1,node:fallback(../DioPortBitNumber,0)) - 1), node:fallback(../DioPortOffset,1))
                                                                )
                                                            )
                                                        )"
                                                        true="This value is wrong. DioPortMask is calculated by formula in the description. Please press the button to calculate the value automatically."/>
                                                         
                                                       <mt:xpath expr="(node:fallback(node:value(../../../../../../DioGeneral/DioReversePortBits), 'false') = 'false')  and ((bit:and ((node:fallback(., 1)),bit:not(num:hextoint(substring(ecu:get('Dio.AvailablePortPinsForWrite'),node:value(../../../DioPortId)*11 + 1 ,10))))!=0) and (bit:and ((node:fallback(., 1)),bit:not(num:hextoint(substring(ecu:get('Dio.AvailablePortPinsForRead'),node:value(../../../DioPortId)*11 + 1 ,10))))!=0)) " 
                                                        true="You are trying to write an unavailable channel"/>
                                                       <mt:xpath expr="(node:fallback(node:value(../../../../../../DioGeneral/DioReversePortBits), 'false') = 'true')  and ((bit:and ((node:fallback(., 1)),bit:not(num:hextoint(substring(ecu:get('Dio.AvailablePortPinsForWriteReversedBits'),node:value(../../../DioPortId)*11 + 1 ,10))))!=0) and (bit:and ((node:fallback(., 1)),bit:not(num:hextoint(substring(ecu:get('Dio.AvailablePortPinsForReadReversedBits'),node:value(../../../DioPortId)*11 + 1 ,10))))!=0))" 
                                                        true="You are trying to write an unavailable channel. Please note that ReversePortBits option in DioGeneral is checked."/>
                                                       <mt:range> <mt:tst expr="&gt;=0" /></mt:range>
                                                       <mt:range> <mt:tst expr="&lt;=4294967295" /></mt:range>                                                        
                                                   </a:a>
                                                </v:var>
                                             </v:ctr>
                                        </v:lst>
                                    </v:ctr>
                                </v:lst>
                            </v:ctr>
                            
                            <!--  @implements DioGeneral_Object  -->
                            <v:ctr name="DioGeneral" type="IDENTIFIABLE">
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html><p>
                                                General DIO module configuration parameters.
                                            </p></html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="UUID" value="ECUC:14bebd57-a98c-496c-9b67-0d150cd8719c"/>
                                <!--  @implements DioDevelopmentErrorDetect_Object  -->
                                <v:var name="DioDevErrorDetect" type="BOOLEAN">
                                    <a:a name="LABEL" value="Dio Development Error Detect"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:
                                                <html><p>
                                                    Switches the Development Error Detection and Notification ON or OFF.<br>
                                                    True: Enabled.<br>
                                                    False: Disabled.<br>
                                                </p></html>
                                            ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                         type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantLinkTime</icc:v>
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:068c841e-9f94-429d-b5a5-c19387eefd19"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <!--  @implements DioVersionInfoApi_Object  -->
                                <v:var name="DioVersionInfoApi" type="BOOLEAN">
                                    <a:a name="LABEL" value="Dio Version Info Api"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:
                                                <html><p>
                                                    Adds / removes the service Dio_GetVersionInfo() from the code.<br>
                                                    True - Dio_GetVersionInfo() API is enabled.<br>
                                                    False - Dio_GetVersionInfo() API is disabled (it cannot be used).<br>
                                                </p></html>
                                            ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                         type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantLinkTime</icc:v>
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:702473f2-8a23-47a9-a347-62f97e4f4114"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <!--  @implements DioReversePortBits_Object  -->
