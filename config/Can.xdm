                                    <a:da name="EDITABLE" type="XPath" expr="../../CanObjectType = 'RECEIVE'"/>
                                    <a:da name="DEFAULT" value="0"/>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&gt;=0"/>
                                        <a:tst expr="&lt;=4294967295"/>
                                    </a:da>
                                </v:var>
                            </v:ctr>
                            
